{
  "name": "sennder-service",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "start": "sls offline start -s dev -r ap-southeast-2",
    "ci": "yarn install --frozen-lockfile",
    "deploy:serverless": "sls deploy",
    "package:serverless": "sls package",
    "test": "jest",
    "lint": "eslint --ext js,ts . --cache --cache-location ./node_modules/.cache/.eslintcache/",
    "lint:fix": "yarn lint --fix",
    "prettier:fix": "prettier --write '**/*.{js,ts,md}' --cache",
    "prettier:check": "prettier --check '**/*.{js,ts,md}' --cache",
    "prepare": "husky install",
    "pre-commit": "npx lint-staged",
    "pre-push": "yarn lint && npx tsc --noEmit"
  },
  "lint-staged": {
    "*.{md,scss}": [
      "prettier --write --cache"
    ],
    "*.{js,ts}": [
      "eslint --fix --cache --cache-location ./node_modules/.cache/.eslintcache/",
      "prettier --write --cache"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.1445.0",
    "lodash.pick": "^4.4.0",
    "source-map": "^0.7.4",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.4",
    "@types/jest-when": "^3.5.2",
    "@types/lodash.pick": "^4.4.7",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "datadog-lambda-js": "^7.98.0",
    "dd-trace": "^4.16.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-circus": "^29.6.4",
    "jest-when": "^3.6.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.3.3",
    "serverless": "^3.34.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-plugin-datadog": "^5.48.0",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-webpack": "^5.13.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "volta": {
    "node": "18.18.0",
    "yarn": "1.22.10"
  }
}
